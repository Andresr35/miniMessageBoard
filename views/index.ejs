<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <link rel="stylesheet" href="/stylesheets/style.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    <ul>
      <% messages.forEach(function(message){ %>
      <li>
        Text from <%= message.user %> on <%= message.added %>: <%= message.text
        %>
      </li>
      <% }) %>
    </ul>
  </body>
</html>
